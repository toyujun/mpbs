<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mplbs.domain.DesPart" table="desnpart">
        <id name="id" type="java.lang.Integer">
            <column name="id" length="4" />
            <generator class="identity" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="50" />
        </property>
        <property name="drawing" type="java.lang.String">
            <column name="drawing" length="50" />
        </property>
		<property name="descb" type="java.lang.String">
            <column name="descb" length="50" />
        </property>
        <property name="creater" type="java.lang.String">
            <column name="creater" length="50" />
        </property>
        <property name="sta" type="java.lang.Integer">
            <column name="sta" length="4" />
        </property>
        <property name="time" type="java.lang.Date">
            <column name="time" />
        </property>
    </class>
</hibernate-mapping>
